{
  "name": "AuthCenter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node --harmony_object_rest_spread bootstrap.js",
    "resolve": "cnpm i --registry=https://npm.finogeeks.club",
    "lint": "./node_modules/.bin/eslint *.js",
    "test": "mocha --timeout 10000",
