    "cookie-parser": "^1.3.3",
    "cors": "^2.7.1",
    "express": "^4.9.0",
    "express-session": "^1.9.0",
    "express-validator": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "ldapjs": "^1.0.1",
    "moment": "^2.8.3",
    "mongoose": "4.10.8",
    "morgan": "^1.3.0",
    "node-client": "^0.1.16",
    "underscore": "^1.8.3",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
